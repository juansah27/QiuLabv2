# Fallback API URL jika autodiscovery gagal
# Ini akan dideteksi otomatis oleh sistem
VITE_API_URL=

# Timeout untuk permintaan API (dalam milidetik) - lebih lama di production
VITE_AXIOS_TIMEOUT=60000

# Mode production
VITE_APP_MODE=production

# Fitur debugging dinonaktifkan di production
VITE_DEBUG=false
VITE_VERBOSE_LOGGING=false
